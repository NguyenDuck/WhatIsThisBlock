{
	"inv_slot": {
		"type": "panel",
		"$index|default": 17,
		"collection_index": "$index",
		"visible": false,
		"bindings": [
			{
				"binding_name": "#item_id_aux",
				"binding_type": "collection",
				"binding_collection_name": "hotbar_items",
				"binding_condition": "visibility_changed"
			},
			{
				"binding_name": "#hud_subtitle_text_string",
				"binding_name_override": "#text"
			},
			{
				"binding_name": "#hud_subtitle_text_string",
				"binding_name_override": "#current",
				"binding_condition": "visibility_changed"
			},
			{
				"binding_type": "view",
				"source_property_name": "(not (#text = #current)) and not ((#text - $block_name_const) = #text))",
				"target_property_name": "#visible"
			}
		]
	},
	"witb": {
		"type": "image",
		"size": ["100%cm", "100%cm"],
		"anchor_from": "top_right",
		"anchor_to": "top_right",
		"texture": "textures/ui/purpleBorder",
		"visible": false,
		"controls": [
			{
				"block_renderer": {
					"type": "custom",
					"renderer": "inventory_item_renderer",
					"size": [16, 16],
					"offset": ["6px", "6px"],
					"anchor_from": "top_left",
					"anchor_to": "top_left",
					"controls": [
						{
							"stack": {
								"type": "stack_panel",
								"size": [0, 0],
								"collection_name": "hotbar_items",
								"controls": [
									{
										"slot@hud.inv_slot": {}
									}
								],
								"bindings": [
									{
										"binding_type": "view",
										"source_control_name": "slot",
										"resolve_sibling_scope": true,
										"source_property_name": "#item_id_aux",
										"target_property_name": "#item_id_aux"
									}
								]
							}
						}
					],
					"bindings": [
						{
							"binding_type": "view",
							"source_control_name": "stack",
							"source_property_name": "#item_id_aux",
							"target_property_name": "#item_id_aux"
						}
					]
				}
			},
			{
