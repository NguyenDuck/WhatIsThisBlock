							"block_namespace": {
								"type": "label",
								"font_scale_factor": 0.8,
								"text": "#text",
								"offset": "$offset",
								"localize": true,
								"anchor_from": "top_left",
								"anchor_to": "top_left",
								"controls": [
									{
										"get_block_namespace": {
											"type": "label",
											"size": [0, 0],
											"text": "#text",
											"visible": false,
